{
    "manifest_version": 2,
    "name": "Vaccine Booker",
    "description": "Testing a vaccine booker",
    "version": "1.0.0",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
    }],
    "browser_action": {
        "default_icon": "/icon128.png",
        "default_popup": "popup.html"
    }
}