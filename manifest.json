{
    "manifest_version": 2,
    "name": "Vaccine Booker",
    "description": "Testing a vaccine booker",
    "version": "1.0.0",
    "icons": {
        "16": "/imgs/icon16.png",
        "48": "/imgs/icon48.png",
        "128": "/imgs/icon128.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_icon": "/imgs/icon128.png",
        "default_popup": "popup.html"
    }
}